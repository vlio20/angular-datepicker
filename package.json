{
  "name": "ng2-date-picker",
  "author": "Vlad Ioffe",
  "version": "2.9.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:headless": "HEADLESS=true ng e2e",
    "build:index": "cd build-helpers && node index-maker.js",
    "build:demo": "rm -rf dist/ && ng build --prod --aot=false --build-optimizer=false --base-href /angular-datepicker/ && npm run build:index",
    "build:prod": "ng-packagr -p package.json",
    "release": "npm run build:prod && npm publish bin",
    "pack": "cd bin && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vlio20/angular-datepicker.git"
  },
  "private": false,
  "keywords": [
    "angular2",
    "date",
    "datepicker",
    "date-picker",
    "ng2-datepicker",
    "ng-datepicker",
    "angular",
    "picker",
    "angular-datepicker"
  ],
  "peerDependencies": {
    "moment": "^2.17.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/cli": "6.0.1",
    "@angular/common": "6.0.1",
    "@angular/compiler": "6.0.1",
    "@angular/compiler-cli": "6.0.1",
    "@angular/core": "6.0.1",
    "@angular/forms": "6.0.1",
    "@angular/platform-browser": "6.0.1",
    "@angular/platform-browser-dynamic": "6.0.1",
    "@angular/router": "6.0.1",
    "@types/google.analytics": "0.0.33",
    "@types/jasmine": "^2.8.8",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "^4.0.2",
    "core-js": "^2.4.1",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less": "^2.7.2",
    "ncp": "^2.0.0",
    "ng-packagr": "^3.0.0-rc.4",
    "protractor": "~5.1.0",
    "rimraf": "^2.6.1",
    "rxjs": "^6.1.0",
    "ts-node": "^4.0.2",
    "tslint": "~5.8.0",
    "typescript": "2.7.2",
    "uglify-js": "^2.8.12",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.26"
  },
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/app/index.ts",
      "externals": {
        "moment": "moment"
      }
    },
    "dest": "bin",
    "whitelistedNonPeerDependencies": [
      "moment"
    ]
  },
  "dependencies": {
    "moment": "^2.22.1"
  }
}
